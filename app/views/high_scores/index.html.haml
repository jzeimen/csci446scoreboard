%h1 Listing high_scores
%h2= link_to "Play The Game!", "/game.html"
%table
  %tr
    %th Name
    %th Score
    %th
    %th
    %th
  - @high_scores.each do |high_score|
    %tr
      %td= high_score.name
      %td= high_score.score
      %td= link_to 'Show', high_score
      %td= link_to 'Edit', edit_high_score_path(high_score)
      %td= link_to 'Destroy', high_score, confirm: 'Are you sure?', method: :delete
%br/
= link_to 'New High score', new_high_score_path
